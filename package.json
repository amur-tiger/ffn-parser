{
  "name": "ffn-parser",
  "version": "0.0.8",
  "description": "A collection of functions to help parse pages off of FanFiction.net",
  "license": "MIT",
  "author": "Arne 'TigeR' Linck",
  "repository": "github:amur-tiger/ffn-parser",
  "keywords": [
    "fanfiction",
    "parser"
  ],
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest src",
    "types": "tsc --noEmit",
    "prebuild": "run-s clean",
    "build": "tsc",
    "prepublishOnly": "run-s build"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.30",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  }
}
